{{#unless part.data}}
    <h2>no se encontr√≥ resultados</h2>
{{/unless}}
{{#each part.data}}
    <div class="wrapper-cards" key={{this._id}} private={{this.privacity}}>
    <img src="{{this.image}}" alt="not image found">
    <h1>{{this.title}}</h1>
    <p class="description">{{this.description}}</p>
    <span id="content-links-downloads" data-text="descargas">
        {{#each this.links}}
            <a href="{{urls this "1"}}" data-text="{{this}}" target="_blank">{{urls this "0"}}</a>
        {{/each}}
    </span>
    {{#if ../part.edit}}
        <span class="operation" id="operation-cards">
            <a href="#modal" data="{{json this}}"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>
            <a href="#delete" data="{{this._id}}" ><i class="fa fa-trash-o" aria-hidden="true"></i></a>
        </span>
    {{/if}}
    <span class="content-info">
        <span>
            <h6>{{propertyUser this.user}}</h6>
            <p>{{split this.date "T" "0"}}</p>
        </span>
        <span class="dis-likes" key="">
            <span class="like">
                <p>0</p>
                <input type="radio" id="{{likes this._id "like"}}" name="{{likes this._id "dis-likes"}}" value="1">
                <label for="{{likes this._id "like"}}"><i class="fa fa-thumbs-up" aria-hidden="true"></i></label>
            </span>
            <span class="dislike">
                <p>0</p>
                <input type="radio" id="{{likes this._id "dislike"}}" name="{{likes this._id "dis-likes"}}" value="0">
                <label for="{{likes this._id "dislike"}}"><i class="fa fa-thumbs-down" aria-hidden="true"></i></label>
            </span>
        </span>
    </span>
</div>
{{/each}}